<trackGroup className="org.utgenome.gwt.utgb.client.track.TrackGroup" name="human">
  <property key="name">human</property>
  <groupProperties>
    <property key="revision">hg19</property>
    <property key="species">human</property>
    <property key="target">chr21</property>
    <property key="dbGroup">org/utgenome/leo/bss/20091014/bss_all</property>
    <property key="dbName">unique_similarity95_coverage80.sqlite</property>
    <trackWindow start="11013222" end="11023222" width="700"/>
  </groupProperties>
  <track className="org.utgenome.gwt.utgb.client.track.lib.NavigatorTrack" name="UTGB Navigator" height="41" pack="true">
      <property key="sequenceList">[{"species":"human", "revision":["hg19","hg18","hg17"]},{"species":"mouse", "revision":["mm9"]},{"species":"drosophila", "revision":["dm3"]}]</property>
  </track>
  <track className="org.utgenome.gwt.utgb.client.track.lib.KeywordSearchTrack" name="Keyword Search" pack="true">
   <property key="keyword.examples">["Scaffold number (e.g. scaffold123)","GenBank accession number (e.g. BJ013363)","UniGene cluster identifier (e.g. Ola.5710)","Clone number (e.g. MF01SSA159G06)"]</property>
   <property key="species.scope">any</property>
  </track>

  <track className="org.utgenome.gwt.utgb.client.track.lib.DBSelectTrack" name="DB Selector" pack="true">
    <property key="favoriteGroup">["org/utgenome/leo/bss/20091119/bss_all", "org/utgenome/leo/bss/20091014/bss_all"]</property>    
  </track>
 
  <track className="org.utgenome.gwt.utgb.client.track.lib.ChromosomeMapTrack" name="Chromosome Map" pack="true">
    <property key="type">image</property>
    <property key="trackBaseURL">utgb-core/ChromosomeMap</property>
    <property key="displayType">rotate</property>
  </track>
  
  <track className="org.utgenome.gwt.utgb.client.track.lib.SequenceRulerTrack" name="Sequence Overview" height="14" pack="true">
    <property key="leftMargin">100</property>
    <property key="ruler.length">300000000</property>
  </track>
  <track className="org.utgenome.gwt.utgb.client.track.lib.RulerTrack" name="Ruler" height="14" pack="true">
    <property key="leftMargin">100</property>
  </track>

  <track className="org.utgenome.gwt.utgb.client.track.lib.GeneCanvasTrack" name="Refseq Genes" pack="true">
     <property key="changeParamOnClick">refseq.query</property>
     <property key="openURLOnClick">http://www.ncbi.nlm.nih.gov/entrez/viewer.fcgi?val=%q</property>
  </track>
 
  <track className="org.utgenome.gwt.utgb.client.track.lib.GenomeTrack" name="Sequence" pack="true"> 
    <property key="type">image</property> 
    <property key="leftMargin">100</property> 
    <property key="trackBaseURL">utgb-core/Sequence.png?%q</property> 
  </track> 

  <track className="org.utgenome.gwt.utgb.client.track.lib.BSSCanvasTrack" name="BSS 454 Reads" pack="true">
    <property key="changeParamOnClick">bss.query</property>
  </track>

  <track className="org.utgenome.gwt.utgb.client.track.lib.GenomeTrack" name="BSS Read Alignment" height="200" pack="true"> 
    <property key="type">frame</property> 
    <property key="leftMargin">0</property> 
    <property key="trackBaseURL">utgb-core/BSSReadView</property> 
    <property key="queryParams">bss.query,dbGroup,dbName</property>
    <property key="monitorCoordinateChange">false</property>
    <property key="monitorWindowChange">false</property>
  </track> 

 



</trackGroup>
