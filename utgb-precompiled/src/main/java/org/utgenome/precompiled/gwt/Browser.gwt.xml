<module rename-to="utgb">
	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>
	<!-- Inherit the UTGB modules -->
	<inherits name="org.utgenome.gwt.utgb.UTGB"/>

	<!-- Specify the app entry point class.                   -->
	<entry-point class='org.utgenome.precompiled.gwt.client.Browser'/>
  
   <servlet path="/service" class="org.utgenome.gwt.utgb.server.BrowserServiceImpl"/>
   <servlet path="/rpc" class="org.utgenome.precompiled.gwt.server.RPCServiceImpl"/>
      
</module>